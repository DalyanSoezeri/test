            <label for="inputEmail" class="sr-only">Email address</label>
            <input type="email" id="username" name="username" class="form-control" alt="enter your Email address" placeholder="Email Adresse"  required autofocus>
            <label for="inputPassword" class="sr-only">Password</label>
            <input type="password" name="password" class="form-control" placeholder="Passwort" alt="Passwort" required>
            <div class="checkbox mb-3">
</div>
            <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
            <br>
            <p class="signup">Dont have an Account? <a href="/sign-up">sign up</a></p>
            <p class="mt-5 mb-3 text-muted">&copy; 2020-2021</p>
        </form>
