  <div class="main">
    <div class="main__left">
      <div class="main__videos">
        <div id="video-grid"></div>
      </div>
      <div class="main__controls">
        <div class="main__controls_block">
          <div
            class="main__controls_button"
            id="muteButton"
            onclick="muteUnmute()"
          >
          <img src="pictures/microphone.png" alt="share link to join" width="25px">
            <span>Mute</span>
          </div>
          <div onclick="playStop()" class="main__controls_button">
            <img src="pictures/videokamera.png" alt="share link to join" width="25px">
      
            <span id="phase">Stop Video</span>
          </div>
        </div>

        <div class="main__controls_block">

          <div class="main__controls_button">
            <img src="pictures/share.png" alt="share link to join" width="25px">
            <span>Participants</span>
          </div>

          <div onclick="displayLocalStreamAndSignal()" class="main__controls_button">
            <img src="pictures/screen.png" alt="share link to join" width="25px">

            <span id="ka">Start ScreenSharing</span>
          </div>

        </div>

        <div class="main__controls_block">
          <div class="main__controls_button leaveMeeting" id="leave-meeting">
            <img src="pictures/cross-sign.png" alt="share link to join" width="25px">
            <span class="">Leave Meeting</span>
          </div>
        </div>
      </div>
    </div>
    
